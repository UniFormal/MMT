<?xml version="1.0"?>

<!DOCTYPE MODE SYSTEM "xmode.dtd">

<MODE>
	<PROPS>
		<PROPERTY NAME="wordBreakChars" VALUE=": " />
	</PROPS>
	<RULES ESCAPE="\"
		IGNORE_CASE="FALSE"
		HIGHLIGHT_DIGITS="FALSE">
		<SEQ TYPE="OPERATOR">info.kwarc.mmt</SEQ>
  		<SEQ TYPE="OPERATOR">:</SEQ>
  		<SEQ TYPE="OPERATOR">(</SEQ>
  		<SEQ TYPE="OPERATOR">)</SEQ>
  		<SEQ TYPE="OPERATOR">.</SEQ>
		<KEYWORDS>
			<KEYWORD1>error</KEYWORD1>
			<KEYWORD1>user</KEYWORD1>
			<KEYWORD2>controller</KEYWORD2>
			<KEYWORD2>presenter</KEYWORD2>
			<KEYWORD2>extman</KEYWORD2>
			<KEYWORD2>archive</KEYWORD2>
		</KEYWORDS>
	</RULES>
</MODE>
