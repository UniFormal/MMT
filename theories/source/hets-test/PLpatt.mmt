%namespace "http://cds.omdoc.org/hets-test".
%namespace found = "http://cds.omdoc.org/foundations".

%sig PLpatt: found.LF = {
  bool : type.
  or : bool → bool → bool # 1 ∨ 2 prec 10.
  and : bool → bool → bool # 1 ∧ 2 prec 10.
  not : bool → bool # ¬ 1 prec 5.
  true : bool.
  false : bool.
  ded : bool → type # ded 1 prec 100.

  %pattern prop = { p : bool.  }.

  %pattern axiom = [ F : bool ] { ax : ded F. } # asserts 1 prec 1000.

}.
