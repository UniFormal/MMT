<catalog>
  <!-- docs/mods can be merged now: use mmt:lookup, mmt:getNotations, add mmt:getAssertions as well -->
  <ombase scheme="http" authority="cds.omdoc.org" prefix="/"
          ombase="http://srv.tntbase.org/tntbase/cds/restful/">
