<?xml version="1.0"?>
<Ontology 
     xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://example.com/owl/families/"
     ontologyIRI="http://example.com/owl/families">
  
 <Declaration>
  <Class IRI="Person"/>
 </Declaration>
   
 <Declaration>
  <Class IRI="Woman"/>
 </Declaration>

 <Declaration>
  <Class IRI="Human"/>
 </Declaration>
 
 <Declaration>
  <Class IRI="Mother"/>
 </Declaration>
    
 <Declaration>
  <Class IRI="Father"/>
 </Declaration>
    
 <Declaration>
  <Class IRI="Parent"/>
 </Declaration>
 
 <Declaration>
  <Class IRI="Man"/>
 </Declaration>
 
 <SubClassOf>
  <Class IRI="Woman"/>
  <Class IRI="Person"/>
 </SubClassOf>
 
 <EquivalentClasses>
  <Annotation>
   <AnnotationProperty IRI="http://omdoc.org/id"/>
   <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#string">id1</Literal>
  </Annotation>
  <Class IRI="Parent"/>
  <ObjectUnionOf>
   <Class IRI="Mother"/>
   <Class IRI="Father"/>
  </ObjectUnionOf>
 </EquivalentClasses>
 
</Ontology>

