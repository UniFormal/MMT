   </notation>
   <notation for="?fol?bot" role="constant" inherit="yes">
      <text value="&#x22a5;"/>
   </notation>
   <notation for="?fol?and" role="constant" inherit="yes">
      <text value="&#x2227;"/>
   </notation>
   <notation for="?fol?or" role="constant" inherit="yes">
      <text value="&#x2228;"/>
   </notation>
   <notation for="?fol?imp" role="constant" inherit="yes">
      <text value="&#x21d2;"/>
   </notation>
   <notation for="?fol?not" role="constant" inherit="yes">
      <text value="&#x00ac;"/>
   </notation>
   <notation for="?fol?forall" role="constant" inherit="yes">
