<?xml version="1.0" encoding="UTF-8"?>
<omdoc xmlns="http://omdoc.org/ns" xmlns:om="http://www.openmath.org/OpenMath">
<style name="hoas-quantifiers" for="http://cds.omdoc.org/foundations/lf/lf.omdoc">
   <include from="mathml.omdoc?mathml"/>
</style>

 
<style name="math-quantifiers" for="http://cds.omdoc.org/foundations/lf/lf.omdoc">
   <include from="mathml.omdoc?mathml"/>
   <notation for="?lf?lambda" role="constant"/>
   <notation for="?lf?lambda" role="binding" precedence="100">
       <components begin="1" end="-2">
         <body><recurse precedence="-infinity"/></body>
         <separator><element name="mo"><text value=","/></element></separator>
       </components>
       <element name="mo"><text value="."/></element>
       <component index="-1" precedence="-88"/>
   </notation>
   <notation for="?lf?oftype" role="attribution" precedence="0">
       <component index="1"/>
   </notation>
</style>

<style name="mathml" for="http://cds.omdoc.org/logics/first-order/fol/fol.omdoc">

