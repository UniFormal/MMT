<ACTIONS>
	<ACTION NAME="mmt-dockable-show">
		<CODE>
		   wm.addDockableWindow("mmt-dockable");
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-compile">
		<CODE>
		   jEdit.getPlugin("info.kwarc.mmt.jedit.MMTPlugin", true).compileActions().compileCurrent(view);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-show">
		<CODE>
		   info.kwarc.mmt.jedit.Hider.show(textArea);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-hide">
		<CODE>
		   info.kwarc.mmt.jedit.Hider.hide(textArea);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-hideCurrent">
		<CODE>
		   info.kwarc.mmt.jedit.Hider.hideCurrent(textArea);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-makeHidable">
		<CODE>
		   info.kwarc.mmt.jedit.Hider.makeHidable(textArea);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-hideshow">
		<CODE>
		   info.kwarc.mmt.jedit.Hider.hideshow(textArea);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-insert-US">
		<CODE>
		   info.kwarc.mmt.jedit.Inserter.insertUSorTab(textArea);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-insert-RS">
		<CODE>
		   info.kwarc.mmt.jedit.Inserter.insertRSReturn(textArea);
		</CODE>
	</ACTION>
	<ACTION NAME="mmt-insert-GS">
		<CODE>
		   info.kwarc.mmt.jedit.Inserter.insertGSReturn(textArea);
		</CODE>
	</ACTION>
</ACTIONS>
