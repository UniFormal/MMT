  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#Import">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#ConstitutiveStatement"/>
    <rdfs:comment>An import from one theory into another one, modeled as a first-class object</rdfs:comment>
    <rdfs:label xml:lang="de">Import</rdfs:label>
    <rdfs:label xml:lang="en">Import</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
