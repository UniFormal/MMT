<?xml version="1.0"?>
<Ontology 
     xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://example.com/owl/families/"
     ontologyIRI="http://example.com/owl/families">
     
    <Declaration>
        <Class IRI="Person"/>
    </Declaration>
    
    <Declaration>
        <DataProperty IRI="Age"/>
    </Declaration>
    
    <Declaration>
        <DataProperty IRI="hasAdress"/>
    </Declaration>
    
    <Declaration>
        <DataProperty IRI="hasAge"/>
    </Declaration>
    
    <Declaration>
        <DataProperty IRI="hasHusband"/>
    </Declaration>
    
    <Declaration>
        <DataProperty IRI="hasLastName"/>
    </Declaration>
    
    <Declaration>
        <DataProperty IRI="hasName"/>
    </Declaration>
    
    <EquivalentDataProperties>
        <DataProperty IRI="Age"/>
        <DataProperty IRI="hasAge"/>
    </EquivalentDataProperties>
    
    <SubDataPropertyOf>
        <DataProperty IRI="hasLastName"/>
        <DataProperty IRI="hasName"/>
    </SubDataPropertyOf>
    
    <FunctionalDataProperty>
        <DataProperty IRI="hasHusband"/>
    </FunctionalDataProperty>
    
    <DataPropertyDomain>
        <DataProperty IRI="hasAge"/>
        <Class IRI="Person"/>
    </DataPropertyDomain>
    
    <DataPropertyRange>
        <DataProperty IRI="hasAge"/>
        <Datatype abbreviatedIRI="xsd:integer"/>
    </DataPropertyRange>
    
    <DisjointDataProperties>
        <DataProperty IRI="hasAdress"/>
        <DataProperty IRI="hasName"/>
    </DisjointDataProperties>
    
</Ontology>




