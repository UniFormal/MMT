<?xml version="1.0"?>
<Ontology xmlns="http://www.w3.org/2002/07/owl#" xml:base="http://example.com/owl/families/" ontologyIRI="http://example.com/owl/families">

	<Declaration>
		<Class IRI="Man"/>
	</Declaration>
	
	<Declaration>
		<Class IRI="Person"/>
	</Declaration>
	
	<SubClassOf>
		<Annotation>
			<AnnotationProperty IRI="http://www.w3.org/2000/01/rdf-schema#comment"/>
			<Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#string">"States that every man is a person."</Literal>
		</Annotation>
		<Class IRI="Man"/>
		<Class IRI="Person"/>
	</SubClassOf>
	
	<AnnotationAssertion>
		<AnnotationProperty IRI="http://www.w3.org/2000/01/rdf-schema#comment"/>
		<IRI>Person</IRI>
		<Literal>Represents the set of all people.</Literal>
	</AnnotationAssertion>

</Ontology>	

